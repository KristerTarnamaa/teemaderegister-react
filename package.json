{
  "name": "teemaderegister-react",
  "version": "0.0.1",
  "description": "Topic registry UI for TLU",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && NODE_ENV=production webpack -p --progress --config webpack.config.js",
    "build-visualization": "npm run clean && NODE_ENV=production visualization=true webpack -p --progress --config webpack.config.js",
    "dev": "npm run clean && NODE_ENV=development webpack-dev-server --hot --inline --debug --config webpack.config.js",
    "lint": "eslint src/",
    "clean": "rm -rf dist/",
    "prod": "npm run lint && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romilrobtsenkov/teemaderegister-react.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Romil Robtsenkov",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/romilrobtsenkov/teemaderegister-react/issues"
  },
  "homepage": "https://github.com/romilrobtsenkov/teemaderegister-react#readme",
  "dependencies": {
    "antd": "^2.12.5",
    "axios": "^0.16.2",
    "moment": "^2.18.1",
    "nprogress": "^0.2.0",
    "query-string": "^4.3.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-import": "^1.3.1",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bundle-loader": "^0.5.5",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.4.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
